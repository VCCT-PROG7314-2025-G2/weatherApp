<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude weather database from cloud backup -->
        <exclude domain="database" path="weather_database"/>
    </cloud-backup>
    <device-transfer>
        <!-- Allow device transfer of all data -->
        <include domain="database" path="weather_database"/>
    </device-transfer>
</data-extraction-rules>
